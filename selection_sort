void selectionSort(vector<int>& arr, int n)
{   
    // Write your code here.
    int temp;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[j]<arr[i]){
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
//     for(int i=0;i<n;i++){
//         cout<<arr[i]<<" "    
//     }cout<<endl;
}

